'''
inp_tot = int(input(''))
for _ in range(0, inp_tot):
	inp = input('').split()
	s = int(inp[2])
	i = s
	upper_limit = int(inp[1])
	lower_limit = int(inp[0])
	while True:
		if i >= lower_limit and i <= upper_limit:
			pass
		else:
			print(i)
			break
		i = i + s